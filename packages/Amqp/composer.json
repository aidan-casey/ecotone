{
  "name": "ecotone/amqp",
  "license": "MIT",
  "homepage": "https://docs.ecotone.tech/",
  "type": "library",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Dariusz Gafka",
      "email": "dgafka.mail@gmail.com"
    }
  ],
  "keywords": [
    "amqp",
    "rabbitmq",
    "rabbit",
    "messaging",
    "asynchronous",
    "message",
    "enterprise integration patterns",
    "eip",
    "distributed architecture",
    "domain driven design"
  ],
  "description": "Extends Ecotone with AMQP integration",
  "autoload": {
    "psr-4": {
      "Ecotone\\Amqp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\Ecotone\\Amqp\\": [
        "tests"
      ]
    }
  },
  "require": {
    "ext-amqp": "*",
    "ecotone/enqueue": "2.0.0-dev",
    "enqueue/amqp-ext": "^0.10.4",
    "enqueue/dsn": "*",
    "enqueue/enqueue": "^0.10.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "behat/behat": "^3.10",
    "phpstan/phpstan": "^1.8",
    "doctrine/annotations": "^1.13"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.32.2-dev"
    }
  }
}
